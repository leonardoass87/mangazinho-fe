{
  "name": "mangazinho-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 4000",
    "build:dev": "cross-env NODE_ENV=development next build",
    "start:dev": "cross-env NODE_ENV=development next start -p 4000",

    "homolog": "cross-env NODE_ENV=homolog next build && cross-env NODE_ENV=homolog next start -p 4000",
    "prod": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production next start -p 4000",

    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "node check-config.js"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "@eslint/eslintrc": "^3",
    "cross-env": "^7.0.3"
  }
}
