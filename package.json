{
  "name": "mangazinho-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "cross-env NODE_ENV=development env-cmd -f .env.local next dev -p 3000",
    "dev": "cross-env NODE_ENV=development env-cmd -f .env.development next dev -p 3000",
    "build": "cross-env NODE_ENV=production env-cmd -f .env.production next build",
    "start": "cross-env NODE_ENV=production env-cmd -f .env.production next start -p 3000",
    "build:homolog": "cross-env NODE_ENV=production APP_ENV=homolog env-cmd -f .env.homolog next build",
    "start:homolog": "cross-env NODE_ENV=production APP_ENV=homolog env-cmd -f .env.homolog next start -p 3000"
  },
  "dependencies": {
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4.1.13"
  }
}
